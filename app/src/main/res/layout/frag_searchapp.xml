<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".searchcontent.SearchActivity">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include
            android:id="@+id/toolbar_SearchApp"
            layout="@layout/toolbar"/>

    </com.google.android.material.appbar.AppBarLayout>
    <TextView
        android:id="@+id/title_text_searchApp"
        android:layout_below="@+id/appbar"
        android:text="@string/history"
        android:fontFamily="@font/robo"
        android:textSize="20sp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclview_searchApp"
        android:layout_below="@+id/title_text_searchApp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</RelativeLayout>